{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "post_type": {
      "type": "enumeration",
      "enum": [
        "build",
        "blog"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "skills": {
      "type": "json"
    },
    "class": {
      "model": "class",
      "via": "posts"
    },
    "author": {
      "via": "posts",
      "plugin": "users-permissions",
      "model": "user"
    },
    "votes": {
      "via": "upvoted_posts",
      "plugin": "users-permissions",
      "collection": "user"
    }
  }
}
